// Place your key bindings in this file to override the defaults
[
    // new terinal i3wm binding
    { "key": "ctrl+enter", "command": "workbench.action.terminal.toggleTerminal"},
    // Toggle between terminal and editor focus
    { "key": "ctrl+space", "command": "workbench.action.terminal.focus"},
    { "key": "ctrl+space", "command": "workbench.action.focusActiveEditorGroup", "when": "terminalFocus"},
    // vim-tmux-navigator
    { "key": "ctrl+h", "command": "workbench.action.focusPreviousGroup"},
    { "key": "ctrl+l", "command": "workbench.action.focusNextGroup"},
]